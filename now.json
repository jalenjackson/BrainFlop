{
  "name": "BrainFlop",
  "alias": "brainflop.com",
  "version": 2,
  "builds": [
    { "src": "next.config.js", "use": "@now/next" },
    { "src": "package.json", "use": "@now/node-server" }
  ],
  "routes": [
    { "src": "/category/(.*)", "dest": "/category?slug=$1" },
    { "src": "/quiz/(.*)/(.*)", "dest": "/quiz?quizName=$1&quizId=$1" },
    { "src": "/single-player/answer-choice/(.*)/(.*)", "dest": "/singlePlayerAnswerChoiceQuizGame?quizName=$1&quizId=$1" },
    { "src": "/invite-friend/answer-choice/(.*)/(.*)/(.*)", "dest": "/inviteAFriendAnswerChoiceQuizGame?quizName=$1&quizId=$1&userId=$1" },
    { "src": "/login", "dest": "/login" },
    { "src": "/profile", "dest": "/profile" },
    { "src": "/personality-quiz/(.*)/(.*)", "dest": "/personalityQuizGame?quizName=$1&quizId=$1" },
    { "src": "/online/answer-choice/(.*)/(.*)", "dest": "/online/answer-choice/$1/$1" },
    { "src": "/create-quiz", "dest": "/createAQuiz" },
    { "src": "/categories", "dest": "/categories" },
    { "src": "/customize-experience", "dest": "/customizeExperience" },
    { "src": "/create-quiz/(.*)", "dest": "/editQuiz" },
    { "src": "/search/(.*)", "dest": "/Search?searchQuery=$1" },
    { "src": "/profile/(.*)/(.*)", "dest": "/viewProfile?name=$1&id=$1" },
    { "src": "/blog/(.*)/(.*)", "dest": "/blog?name=$1&id=$1" },
    { "src": "/personality-quizzes", "dest": "/personalityQuizzes" },
    { "src": "/register", "dest": "/Register" },
    { "src": "/terms-and-conditions", "dest": "/termsAndConditions" },
    { "src": "/privacy-policy", "dest": "/privacyPolicy" },
    { "src": "/reset/(.*)", "dest": "/forgotPassword" }
  ]
}
